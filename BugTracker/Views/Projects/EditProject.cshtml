@model AddEditProjectsViewModel 

@{
    ViewBag.Title = "EditProject";
}

<h2>Edit Project</h2>

<form method="post" enctype="multipart/form-data">
    @Html.ValidationSummary(false)
    <div class="form-group">
        @Html.LabelFor(p => Model.Name, "Project Name:")
        @Html.TextBoxFor(p => Model.Name, new { @class = "form-control", placeholder = "Project Name" })
    </div>
    <div class="">
        <button type="submit" class="btn btn-default">Submit</button>
    </div>
</form>
